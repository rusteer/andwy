<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd"
	default-lazy-init="true">

	<description>ServerSetting</description>
	<bean id="ServerSetting" class="net.andwy.andwyadmin.service.ServerSetting">
		<property name="enableBuild" value="true" />
		<property name="versionType" value="2" />
		<property name="buildPath" value="/tmp/listen"/>
	</bean>
</beans>